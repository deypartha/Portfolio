  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/ScrollTrigger.min.js"></script>
  <script>
    document.addEventListener("DOMContentLoaded", function () {
        gsap.registerPlugin(ScrollTrigger);
        function animateElement(selector, options) {
            gsap.from(selector, {
                opacity: 0,
                scale: 0.9,
                immediateRender: false,
                ...options,
                scrollTrigger: {
                    trigger: selector,
                    start: selector === "#main1" ? "top 90%" : "top 75%", // First section starts earlier
                    end: "bottom 20%",
                    toggleActions: "play none none reset",
                    once: false,
                    markers: false,
                }
            });
        }
        animateElement("#main1", { x: -50, duration: 1, ease: "power3.out" });
        animateElement("#greet-left", { x: -100, duration: 1, ease: "power3.out" });
        animateElement("#greet-right", { x: 100, duration: 1, ease: "power3.out" });
    });


      document.addEventListener("DOMContentLoaded", function () {
        gsap.registerPlugin(ScrollTrigger);

        function animateElement(selector, options) {
            gsap.from(selector, {
                ...options,
                scrollTrigger: {
                    trigger: selector,
                    start: "top 85%",
                    end: "bottom 20%",
                    toggleActions: "play none none reset", // Replay animation when scrolling back up
                    once: false, // Ensures it runs every time
                    markers: false, // Set to true for debugging
                }
            });
        }
        animateElement("#greet-top", { opacity: 0, y: -50, duration: 1, ease: "power3.out" });
        animateElement("#about1", { opacity: 0, x: -100, duration: 1, ease: "power3.out" });
        animateElement("#greet-bottom", { opacity: 0, x: 100, duration: 1, ease: "power3.out" });
    });

    document.addEventListener("DOMContentLoaded",function(){
      gsap.registerPlugin(ScrollTrigger);
      function animateElement(selector){
        gsap.from(selector,{
          opacity: 0,
          y: 50,
          ease: "power3.out",
          duration:1,
          scrollTrigger:{
            trigger: selector,
            start: "top 80%",
            end: "bottom 30%",
            toggleActions: "play none none reset",
            once: false,
            markers: false,
          }
        })
      }
      animateElement("#skill-row1");
      animateElement("#skill-row2");
      animateElement("#skill-row3");
    })
  </script>
  
